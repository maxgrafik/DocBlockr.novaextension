{
    "env": {
        "es2021": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "warn",
        "eqeqeq": "warn",
        "no-var": "warn",
        "prefer-const": "warn",
        "comma-dangle": ["warn", "only-multiline"],
        "indent": ["error", 4],
        "quotes": ["error", "double", { "avoidEscape": true }],
        "semi": ["error", "always"]
    },
    "globals": {
        "ByteLengthQueueingStrategy": "readonly",
        "Charset": "readonly",
        "Color": "readonly",
        "ColorFormat": "readonly",
        "ColorInformation": "readonly",
        "ColorPresentation": "readonly",
        "CompletionItem": "readonly",
        "CompletionColorPop": "readonly",
        "CompletionItemKind": "readonly",
        "CompletionReason": "readonly",
        "CompositeDisposable": "readonly",
        "CountQueueingStrategy": "readonly",
        "Disposable": "readonly",
        "Emitter": "readonly",
        "Headers": "readonly",
        "InsertTextFormat": "readonly",
        "Issue": "readonly",
        "IssueCollection": "readonly",
        "IssueParser": "readonly",
        "IssueSeverity": "readonly",
        "LanguageClient": "readonly",
        "Modules": "readonly",
        "NotificationRequest": "readonly",
        "Process": "readonly",
        "Range": "readonly",
        "ReadableStream": "readonly",
        "Request": "readonly",
        "Response": "readonly",
        "Scanner": "readonly",
        "ScopeSelector": "readonly",
        "Task": "readonly",
        "TaskCommandAction": "readonly",
        "TaskProcessAction": "readonly",
        "TaskResolvableAction": "readonly",
        "TextDecoder": "readonly",
        "TextEdit": "readonly",
        "TextEditor": "readonly",
        "TextEncoder": "readonly",
        "TransformStream": "readonly",
        "TreeItem": "readonly",
        "TreeItemCollapsibleState": "readonly",
        "TreeView": "readonly",
        "WritableStream": "readonly",
        "atob": "readonly",
        "btoa": "readonly",
        "clearInterval": "readonly",
        "clearTimeout": "readonly",
        "console": "readonly",
        "fetch": "readonly",
        "nova": "readonly",
        "self": "readonly",
        "setInterval": "readonly",
        "setTimeout": "readonly",
        "TaskDebugAdapterAction": "readonly"
    }
}
